{
  "name": "nestjs-angular-skeleton",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "build:api": "tsc -p api/tsconfig.build.json",
    "build:ui": "ng build --prod",
    "build": "npm-run-all clean build:ui build api",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "start:api:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p api/tsconfig.build.json\" ",
    "start:ui:dev": "ng serve --proxy-config proxy.conf.json",
    "start:api:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "lint:api": "tslint -p api/tsconfig.json -c api/tslint.json",
    "lint:ui": "ng lint",
    "test:api": "jest",
    "test:api:watch": "jest --watch",
    "test:api:cov": "jest --coverage",
    "test:api:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:api:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/animations": "~7.2.0",
    "@angular/cli": "~7.3.9",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "10.12.18",
    "@types/supertest": "^2.0.7",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "core-js": "^2.5.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "6.3.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslib": "^1.9.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0",
    "zone.js": "~0.8.26"
  },
  "bundleDependencies": [
    "@nestjs/common",
    "@nestjs/core",
    "@nestjs/platform-express",
    "core-js",
    "font-awesome",
    "lodash",
    "reflect-metadata",
    "request",
    "request-promise-native",
    "rxjs"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
